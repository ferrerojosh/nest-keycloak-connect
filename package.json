{
  "name": "nest-keycloak-connect",
  "version": "1.10.0",
  "description": "keycloak-nodejs-connect module for Nest",
  "author": "John Joshua Ferrer <johnjoshuaferrer@disroot.org>",
  "license": "MIT",
  "scripts": {
    "start:dev": "tsc -w",
    "clean": "rimraf dist",
    "build": "tsc",
    "release": "release-it",
    "release:alpha": "release-it --preReleaseId=alpha --npm.tag=next --github.preRelease",
    "format": "prettier src --write",
    "lint": "eslint src",
    "prepare": "husky"
  },
  "keywords": [
    "nestjs",
    "keycloak",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ferrerojosh/nest-keycloak-connect"
  },
  "bugs": "https://github.com/ferrerojosh/nest-keycloak-connect/issues",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "peerDependencies": {
    "@nestjs/common": ">=6.0.0 <11.0.0",
    "@nestjs/core": ">=6.0.0 <11.0.0",
    "@nestjs/graphql": ">=6",
    "keycloak-connect": ">=10.0.0 <26.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/graphql": "^12.1.1",
    "@types/express": "^4.17.21",
    "@types/node": "^18.19.23",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cpr": "3.0.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "graphql": "^16.8.1",
    "husky": "^9.0.11",
    "keycloak-connect": "24.0.1",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "reflect-metadata": "0.2.1",
    "release-it": "17.1.1",
    "rimraf": "3.0.2",
    "rxjs": "7.8.1",
    "ts-node": "10.8.2",
    "typescript": "5.4.2",
    "typescript-eslint": "^7.2.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/graphql": {
      "optional": true
    }
  },
  "funding": [
    {
      "type": "individual",
      "url": "https://www.paypal.me/ferrerojosh/"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/ferrerojosh"
    }
  ],
  "volta": {
    "node": "21.7.1"
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier --write",
      "eslint"
    ]
  }
}
